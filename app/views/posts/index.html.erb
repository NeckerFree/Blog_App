<div id="postTemplate">
   <%= render partial: "shared/single_user", locals: { user: @user } %>
<% @posts.each do |post| %>
     <%= render partial: "shared/single_post", locals: { post: post } %>   
     <%= render partial: "shared/collection_comments", locals: { post: post, user: @user } %>    
<% end %>
<%= button_to "Pagination"%>
 </div>


     